{
  "name": "todo",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "js": "babel es6/gen/lib es6/app/AppUtils.es6 es6/app/ReplayUtils.es6 es6/gen/ace es6/gen/app es6/src es6/gen/elr es6/app/App.es6 --out-file lib/todo.js",
    "uglify": "uglifyjs lib/todo.js --compress --mangle --output lib/todo.min.js",
    "build": "browserify ./es6/src/app/App.js --standalone Todo -o lib/todo.js",
    "start": "watchify ./es6/src/app/App.js --standalone Todo -o lib/todo.js -dv & live-server --host=127.0.0.1 --port=9999 --proxy='/api/':'http://localhost:9090/api/' --proxy='/replay/':'http://localhost:9990/replay/' --browser='Google Chrome'",
    "ace": "watchify es6/src/ace/App.js -o lib/ace.js -dv & live-server --host=127.0.0.1 --port=9998 --proxy='/api/':'http://localhost:9090/api/' --browser='Google Chrome'"
  },
  "author": "Annette Pohl",
  "license": "ISC",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^2.5.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "http-proxy-middleware": "^0.17.4",
    "node-sass": "^4.5.3",
    "serve": "^6.4.1",
    "uglify-js": "^3.1.6",
    "watch": "^1.0.2",
    "watchify": "^3.9.0"
  }
}
